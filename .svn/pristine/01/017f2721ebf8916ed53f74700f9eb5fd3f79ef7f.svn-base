import request from '@/utils/request'
export function api (url, method, data) {
  return request({
    url: url,
    method: method,
    data: data
  })
}
export function api1 (url, method, data) {
  return request({
    url: url,
    method: method,
    data: data,
    responseType: 'arraybuffer'
  })
}
